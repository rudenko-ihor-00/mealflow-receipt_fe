{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "DOCKERFILE",
    "dockerfilePath": "Dockerfile"
  },
  "deploy": {
    "startCommand": "/bin/sh -c 'echo \"🚂 Railway starting...\" && echo \"📁 Container contents:\" && ls -la && echo \"🚀 Executing start.sh...\" && /app/start.sh'",
    "healthcheckPath": "/health",
    "healthcheckTimeout": 300,
    "restartPolicyType": "ON_FAILURE",
    "restartPolicyMaxRetries": 10
  },
  "variables": {
    "NODE_ENV": "production",
    "PORT": "8080"
  },
  "repository": "https://github.com/rudenko-ihor-00/mealflow-receipt_fe"
}
